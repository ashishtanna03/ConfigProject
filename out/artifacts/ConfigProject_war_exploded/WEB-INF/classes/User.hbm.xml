<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="dao.UserDao" table="user" >
        <id name="userName" type="java.lang.String" column="user_name" unsaved-value="0"> </id>

        <property name="userPhNO" type="java.lang.String" column="password"/>
        <property name="userAddress" type="java.lang.String" column="useraddress"/>

    </class>
</hibernate-mapping>